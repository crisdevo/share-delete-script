local Tab = Win:CraftTab('auto')

local Page = Tab:CraftPage('auto',1) -- Name,1 or 2

Page:Button("secret sea 1",function()
local UserInputService = game:GetService("UserInputService")
local running = true  -- Biến kiểm soát vòng lặp

-- Vòng lặp chính
task.spawn(function()
    while running and wait() do
        fireclickdetector(game:GetService("Workspace").MAP["STARTER ISLAND [ Lv 1+ ]"]["Secret Random"].RANDOM)
    end
end)

-- Hàm lắng nghe phím bấmUserInputService.InputBegan:Connect(function(input, gameProcessed)
    if input.KeyCode == Enum.KeyCode.X then  -- Nhấn phím X để dừng vòng lặp
        running = false
        print("Đã dừng vòng lặp!")
    end
end)
end)

